  "description": "Google Ads API Report Fetcher (gaarf)",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "bin": {
    "gaarf": "gaarf",
    "gaarf-bq": "gaarf-bq"
  },
  "type": "commonjs",
  "engines": {
    "node": ">=15.0.0"
  },
